{
  "name": "vue-hackernews-2.0",
  "description": "A Vue.js project",
  "author": "Evan You <yyx990803@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "test": "npm run test:unit",
    "test:e2e": "rimraf src/tests/e2e/reports && cross-env NODE_ENV=testing node src/tests/e2e/runner.js",
    "test:unit": "rimraf src/tests/unit/coverage && cross-env NODE_ENV=testing karma start src/tests/unit/karma.conf.js --single-run --display-error-details"
  },
  "engines": {
    "node": "9.11.1",
    "npm": "5.6.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.1",
    "dotenv": "^5.0.1",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "lru-cache": "^4.1.1",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.16",
    "vue-meta": "^1.5.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "avoriaz": "^6.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chokidar": "^1.7.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "karma": "^2.0.2",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
